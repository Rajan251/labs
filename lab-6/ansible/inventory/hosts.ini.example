[webservers]
web-1 ansible_host=54.123.45.67 ansible_user=ubuntu ansible_ssh_private_key_file=~/.ssh/terraform-key
web-2 ansible_host=54.123.45.68 ansible_user=ubuntu ansible_ssh_private_key_file=~/.ssh/terraform-key

[appservers]
app-1 ansible_host=54.123.45.69 ansible_user=ubuntu ansible_ssh_private_key_file=~/.ssh/terraform-key

[databases]
db-1 ansible_host=10.0.2.30 ansible_user=ubuntu ansible_ssh_private_key_file=~/.ssh/terraform-key

# Group of groups
[production:children]
webservers
appservers
databases

# Variables for all hosts
[all:vars]
ansible_python_interpreter=/usr/bin/python3
ansible_ssh_common_args='-o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null'
environment=production
